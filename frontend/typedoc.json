{
  "tsconfig": "./tsconfig.typedoc.json",
  "entryPoints": [
    "ts/animations",
    "ts/games",
    "ts/services",
    "ts/utils"
  ],
  "entryPointStrategy": "expand",
  "exclude": [
    "**/App.tsx", 
    "**/main.tsx", 
    "**/Header.tsx", 
    "**/components/**", 
    "**/pages/**",
    "**/hooks/**",
    "**/context/**",
    "**/node_modules/**",
    "**/dist/**",
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "readme": "none",
  "json": "../.typedoc-tmp/frontend.json"
}
