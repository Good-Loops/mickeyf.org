{
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "ES2016",
        "module": "esnext",
        "moduleResolution": "bundler",
        "outDir": "public/dist",
        "baseUrl": "./",
        "sourceMap": true,
        "allowJs": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    },
    "include": [
        "**/*.ts"
, "ts/pages/Animations.tsx", "ts/pages/Games.tsx", "ts/pages/Leaderboard.tsx", "ts/pages/Social.tsx", "ts/pages/Login.tsx"    ],
    "exclude": [
        "node_modules"
    ]
}