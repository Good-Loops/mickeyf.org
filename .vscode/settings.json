{
    "restoreTerminals.terminals": [
        {
            "splitTerminals": [
                {
                    "name": "front",
                    "commands": [
                        "cd frontend && npm run dev"
                    ]
                }
            ]
        },
        {
            "splitTerminals": [
                {
                    "name": "general",
                }
            ]
        },
        {
            "splitTerminals": [
                {
                    "name": "docs",
                    "commands": [
                        "npm run docs:dev"
                    ]
                }
            ]
        }
    ],
    "terminal.integrated.defaultProfile.linux": "rbash",
    "editor.inlineSuggest.enabled": true,
    "editor.suggest.preview": true,
    "editor.tabCompletion": "onlySnippets"
}